# Production Environment Variables
NODE_ENV=production
VITE_APP_NAME=Category Challenge
VITE_APP_VERSION=1.0.0
VITE_DEBUG_MODE=false
VITE_ENABLE_ANALYTICS=true
VITE_API_URL=https://api.categorychallenge.com
VITE_ENABLE_DEV_TOOLS=false

# Game-specific production settings
VITE_SHOW_FPS=false
VITE_ENABLE_SCENE_DEBUG=false
VITE_MOCK_WORD_API=false
VITE_FAST_LOAD_ASSETS=false

# Storage configuration
VITE_USE_LOCAL_STORAGE=true
VITE_USE_INDEXED_DB=true
VITE_CLEAR_STORAGE_ON_START=false