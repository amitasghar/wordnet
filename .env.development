# Development Environment Variables
NODE_ENV=development
VITE_APP_NAME=Category Challenge
VITE_APP_VERSION=0.0.0
VITE_DEBUG_MODE=true
VITE_ENABLE_ANALYTICS=false
VITE_API_URL=http://localhost:3001
VITE_ENABLE_DEV_TOOLS=true

# Game-specific development settings
VITE_SHOW_FPS=true
VITE_ENABLE_SCENE_DEBUG=true
VITE_MOCK_WORD_API=true
VITE_FAST_LOAD_ASSETS=true

# Storage configuration
VITE_USE_LOCAL_STORAGE=true
VITE_USE_INDEXED_DB=false
VITE_CLEAR_STORAGE_ON_START=false